# http://www.graphviz.org/Gallery/directed/fsm.gv.txt

digraph pacman_dfsm {

node [shape = circle];
loading [ label = "loading" ];
idle [ label = "idle" ];
ready [ label = "ready" ];
play [ label = "play" ];

loading -> idle;
idle -> ready [ label = "insert coin and press start button" ]
ready -> play [ label = "wait" ];
play -> idle [ label = "no lives left" ];

}
